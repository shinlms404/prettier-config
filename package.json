{
  "name": "@shinlms404/prettier-config",
  "version": "1.0.3",
  "description": "My Prettier config preset",
  "keywords": [
    "typescript",
    "starter",
    "template"
  ],
  "homepage": "https://github.com/shinlms404/prettier-config#readme",
  "bugs": "https://github.com/shinlms404/prettier-config/issues",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/shinlms404/prettier-config.git"
  },
  "license": "MIT",
  "author": "Charlie Chan <shinlms404@gmail.com>",
  "sideEffects": false,
  "type": "module",
  "exports": {
    ".": {
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.js",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist"
  ],
  "scripts": {
    "build": "tsup --format esm,cjs --clean --dts",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prepack": "npm run build",
    "prepare": "npx simple-git-hooks",
    "release": "bumpp && npm publish",
    "stub": "tsup --format esm",
    "test": "vitest",
    "typecheck": "tsc --noEmit",
    "watch": "tsup --format esm,cjs --watch"
  },
  "simple-git-hooks": {
    "pre-commit": "npx lint-staged"
  },
  "lint-staged": {
    "*": "eslint --fix"
  },
  "dependencies": {
    "prettier-plugin-organize-imports": "^3.2.4",
    "prettier-plugin-packagejson": "^2.5.10"
  },
  "devDependencies": {
    "@shinlms404/eslint-config": "^0.1.10",
    "@types/node": "^20.17.23",
    "bumpp": "^9.11.1",
    "cpy-cli": "^5.0.0",
    "lint-staged": "^15.4.3",
    "local-pkg": "^1.1.1",
    "simple-git-hooks": "^2.11.1",
    "sucrase": "^3.35.0",
    "tsup": "^8.4.0",
    "typescript": "^5.8.2",
    "vitest": "^1.6.1"
  },
  "peerDependencies": {
    "eslint": "^8.57.1",
    "prettier": "^3.5.3"
  },
  "publishConfig": {
    "access": "public"
  }
}
